{
    "name": "newelement/dmp-emby",
    "description": "Emby plugin for DMP",
    "license": "GPL-2.0-or-later",
    "homepage": "http://github.com/newelement/dmp-emby",
    "authors": [
        {
            "name": "Don Jones",
            "email": "newelementdesigns@me.com"
        }
    ],
    "require": {
        "newelement/digital-movie-poster": "*"
    },
    "autoload": {
        "psr-4": {
            "Newelement\\DmpEmby\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Newelement\\DmpEmby\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Newelement\\DmpEmby\\DmpEmbyServiceProvider"
            ]
        }
    }
}
